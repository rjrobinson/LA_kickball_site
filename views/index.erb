    <h1>League Data</h1>
<!-- List team names -->
    <ul class="team-names">
    <% @teams.each do |team|  %>
        <li><a href="/<%=team%>"><%=team %></a></li>
        <% end %>
    </ul>

    <h3>View by position</h3>
    <ul>
        <% @position.each do |position|%>
        <li><a href="/allplayers/<%=position%>"><%=position%></a></li>
        <% end %>
    </ul>
